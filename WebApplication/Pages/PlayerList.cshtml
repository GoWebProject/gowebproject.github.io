@page
@using WebApplication.Models
@using Microsoft.AspNetCore.Components
@using Microsoft.AspNetCore.Http
@using Microsoft.AspNetCore.Components.Web;
@model PlayerListModel
@{
    ViewData["Title"] = "Список Участников";
}
<div class="white">
<h1 style="margin-bottom: 30px; align-self: flex-start;">@ViewData["Title"]</h1>

<component type="typeof(PlayerList)" param-SearchQuery="@Request.Query["query"].ToString()" param-LoggedUsername="@Request.Cookies["username"]" render-mode="ServerPrerendered"/>
<script src="~/_framework/blazor.server.js"></script>
</div>